
abstract project Config is

   Root_Source_Dir := "@PROJECT_SOURCE_DIR@";
   Root_Build_Dir  := "@PROJECT_BINARY_DIR@";

   CXX_Compiler_Switches := (@CXX_COMPILER_SWITCHES@);

   Clang_Imported_Location   := "@CLANG_LIBRARY@";
   Qt5Core_Imported_Location := "@Qt5Core_IMPORTED_LOCATION@";

end Config;
